{
  "name": "vaened/php-sequence-generator",
  "description": "Facilitates the generation and autocompletion of a sequential value in the database",
  "keywords": [
    "sequence",
    "sequenceable",
    "succession",
    "autoincrement",
    "increment",
    "vaened",
    "enea"
  ],
  "authors": [
    {
      "name": "enea dhack",
      "email": "enea.so@live.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "lambdish/phunctional": "^2.1"
  },
  "license": "MIT",
  "type": "library",
  "minimum-stability": "stable",
  "require-dev": {
    "phpunit/phpunit": "^9.3.7",
    "symfony/var-dumper": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Vaened\\SequenceGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vaened\\Tests\\": "tests/"
    }
  }
}
